# GSwap Trader Configuration

# Network RPC URLs
VITE_GALA_RPC_URL=https://rpc.galachain.com
VITE_BSC_RPC_URL=https://bsc-dataseed1.binance.org:443
VITE_ETH_RPC_URL=https://eth.llamarpc.com

# Wallet Configuration (for auto-connection)
# WARNING: Never commit your private key to git! Add .env to .gitignore
# If set, the app will automatically connect to this wallet on startup
# Format: 64-character hex string (with or without 0x prefix)
VITE_WALLET_PRIVATE_KEY=

# Trading Configuration
VITE_DEFAULT_RISK_LEVEL=balanced
VITE_DEFAULT_STRATEGY=trend
VITE_DEFAULT_SPEED=normal
VITE_DEFAULT_SIGNALS=normal
VITE_DEFAULT_BIAS=neutral
VITE_DEFAULT_PAPER_TRADING=true
VITE_AUTO_START_TRADING=false

# API Keys (optional)
VITE_COINGECKO_API_KEY=